arrest.Name = "arrest"
arrest.Parent = miscframe
arrest.BackgroundColor3 = Color3.fromRGB(109, 109, 109)
arrest.BackgroundTransparency = 0.600
arrest.BorderColor3 = Color3.fromRGB(109, 109, 109)
