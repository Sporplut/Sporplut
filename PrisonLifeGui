modgun.Name = "modgun"
modgun.Parent = miscframe
modgun.BackgroundColor3 = Color3.fromRGB(109, 109, 109)
modgun.BackgroundTransparency = 0.600
modgun.BorderColor3 = Color3.fromRGB(109, 109, 109)
modgun.Position = UDim2.new(0.671000004, 0, 0.178000003, 0)
modgun.Size = UDim2.new(0, 83, 0, 26)
modgun.Font = Enum.Font.GothamBlack
modgun.Text = "MOD GUNS"
modgun.TextColor3 = Color3.fromRGB(0, 0, 0)
modgun.TextScaled = true
modgun.TextSize = 14.000
modgun.TextWrapped = true
modgun.MouseButton1Down:connect(function()
	local m = require(game:GetService('Players').LocalPlayer.Character:FindFirstChildOfClass("Tool").GunStates)
	m.Damage = 100
	m.MaxAmmo = math.huge
	m.CurrentAmmo = math.huge
	m.AutoFire = true
	m.FireRate = 0
	local message = Instance.new("Message",game.Workspace)
	message.Text = 'MOD GUN ENABLED MAKE SURE YOU HOLD A GUN FIRST BEFORE CLICK "MOD GUNS"'
	wait(4)
	message:Destroy()
end)

